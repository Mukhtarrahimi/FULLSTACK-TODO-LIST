<div class="header">
  <h2>TODO LIST</h2>
</div>
<div class="add-task">
  <h3>Add Task</h3>
  <form action="/" method="post">
    <div class="input">
      <input type="text" placeholder="Enter New Task..." name="title" />
      <input
        type="text"
        placeholder="Enter description..."
        name="description"
      />
    </div>
    <button>Add</button>
  </form>
</div>
<ul class="tasks">
  <% tasks.forEach((task) =>{ %>
  <li>
    <button class="finished"></button>
    <div class="text">
      <h4><%= task.title %></h4>
      <p><%= task.description %></p>
    </div>
    <a class="delete" href="">delete</a>
    <a class="edite" href="">edite</a>
  </li>
  <%})%>
</ul>
